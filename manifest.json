{
  "manifest_version": 2,
  "name": "Flippa Fetch Alexa Websites Rank",
  "version": "1.0.2",
  "minimum_chrome_version": "15",
  "icons": {
    "38": "icon-38x38.png",
    "19": "icon-19x19.png"
  },
  "browser_action": {
    "default_icon": {
      "38": "icon-38x38.png",
      "19": "icon-19x19.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://flippa.com/websites*",
        "*://*.flippa.com/websites*"
      ],
      "js": [
        "start.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs", "activeTab"
  ]
}